{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npx next dev --turbopack",
    "build": "npx next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "npx next start",
    "lint": "npx next lint",
    "type-check": "tsc --noEmit",
    "lint-fix": "npx next lint --fix",
    "pre-commit": "npm run type-check && npm run lint-fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "commit": "cz",
    "test": "echo \"No tests specified\" && exit 0",
    "test:ci": "echo \"No tests specified\" && exit 0",
    "test:supabase": "npx tsx scripts/test-supabase.ts",
    "test:supabase-quick": "npx tsx scripts/quick-test.ts",
    "check:tables": "npx tsx scripts/check-tables.ts",
    "migrate:products": "npx tsx scripts/migrate-to-supabase.ts",
    "generate:keys": "node scripts/generate-keys.js",
    "generate:keys:simple": "node scripts/generate-keys.js --simple",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@heroicons/react": "^2.2.0",
    "@next/third-parties": "^15.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "browser-image-compression": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
